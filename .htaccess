<IfModule mod_rewrite.c>
    Options -Indexes
    RewriteEngine On
    RewriteBase /
    RewriteCond %{REQUEST_URI} \.(db|log|htaccess)$
    RewriteRule ^ - [F]
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^api/(.*)$ page/v2/loader.php?path=$1 [QSA,L]
    RewriteRule ^v2/(.*)$ page/v2/$1.php [QSA,L]
    RewriteRule ^docs/(.*)$ page/manager.php?path=$1 [QSA,L]
</IfModule>
